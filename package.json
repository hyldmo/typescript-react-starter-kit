{
    "name": "typescript-react-starter-kit",
    "repository": "https://github.com/eivhyl/typescript-react-starter-kit",
    "version": "0.1.0",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.development.config.js --progress --colors",
        "production": "webpack --config webpack.production.config.js --progress --colors",
        "test": "jest",
        "lint": "tslint {__tests__,src}/**/*.{ts,tsx} --type-check -p ./",
        "lint:fix": "npm run lint -- --fix"
    },
    "pre-commit": [
        "lint:fix",
        "test"
    ],
    "jest": {
        "transform": {
            ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
            "\\.(css|less|pcss)$": "<rootDir>/__mocks__/styleMock.ts"
        }
    },
    "keywords": [
        "Typescript",
        "React",
        "Redux"
    ],
    "license": "MIT",
    "dependencies": {
        "cross-env": "^3.1.3",
        "history": "^4.6.3",
        "normalize.css": "^5.0.0",
        "react": "^15.4.1",
        "react-dom": "^15.4.1",
        "react-redux": "^4.4.6",
        "react-router": "^4.1.1",
        "react-router-dom": "^4.1.1",
        "react-router-redux": "^5.0.0-alpha.6",
        "redux": "^3.7.2",
        "redux-form": "^7.0.0",
        "redux-saga": "^0.13.0"
    },
    "devDependencies": {
        "@types/history": "^4.6.0",
        "@types/jest": "^20.0.2",
        "@types/node": "^7.0.31",
        "@types/react": "*",
        "@types/react-dom": "*",
        "@types/react-hot-loader": "^3.0.3",
        "@types/react-redux": "*",
        "@types/react-router": "^4.0.14",
        "@types/react-router-dom": "^4.0.6",
        "@types/react-router-redux": "^5.0.3",
        "@types/react-test-renderer": "^15.5.1",
        "@types/redux-actions": "^1.2.2",
        "@types/redux-form": "^6.6.7",
        "@types/webpack": "^2.0.0",
        "@types/webpack-env": "^1.12.1",
        "awesome-typescript-loader": "^3.2.1",
        "css-loader": "^0.26.1",
        "extract-loader": "^0.1.0",
        "extract-text-webpack-plugin": "^2.1.2",
        "file-loader": "^0.9.0",
        "html-webpack-plugin": "^2.29.0",
        "jest": "^20.0.4",
        "postcss": "^6.0.6",
        "postcss-cssnext": "^3.0.0",
        "postcss-import": "^10.0.0",
        "postcss-loader": "^2.0.6",
        "react-hot-loader": "^3.0.0-beta.7",
        "react-redux-typescript": "^2.3.0",
        "react-test-renderer": "^15.6.1",
        "redbox-react": "^1.4.3",
        "source-map-loader": "^0.1.5",
        "style-loader": "^0.13.1",
        "ts-jest": "^20.0.7",
        "tslint": "^5.6.0",
        "tslint-eslint-rules": "^4.1.1",
        "typescript": "~2.4.2",
        "webpack": "^3.1.0",
        "webpack-dev-server": "^2.4.5"
    }
}
